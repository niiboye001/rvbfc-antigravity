{
    "cli": {
        "version": ">= 7.0.0"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "EXPO_PUBLIC_APP_VARIANT": "admin"
            }
        },
        "development-client": {
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "EXPO_PUBLIC_APP_VARIANT": "client"
            }
        },
        "preview": {
            "distribution": "internal",
            "env": {
                "EXPO_PUBLIC_APP_VARIANT": "admin"
            }
        },
        "preview-client": {
            "distribution": "internal",
            "env": {
                "EXPO_PUBLIC_APP_VARIANT": "client"
            }
        },
        "production": {
            "android": {
                "buildType": "apk",
                "image": "latest"
            },
            "env": {
                "EXPO_PUBLIC_APP_VARIANT": "admin"
            }
        },
        "production-client": {
            "android": {
                "buildType": "apk",
                "image": "latest"
            },
            "env": {
                "EXPO_PUBLIC_APP_VARIANT": "client"
            }
        }
    },
    "submit": {
        "production": {}
    }
}